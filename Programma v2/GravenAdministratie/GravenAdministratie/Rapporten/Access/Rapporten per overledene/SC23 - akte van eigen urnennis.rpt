[Query-0]
SQL1=  select Trim(Mid(positie.objvandat,7,2)&'-'&Mid(positie.objvandat,5,2)&'-'&Mid(positie.objvandat,1,4)) as vanafdat,
SQL2=  Trim(Mid(positie.objtemdat,7,2)&'-'&Mid(positie.objtemdat,5,2)&'-'&Mid(positie.objtemdat,1,4)) as temdat, 
SQL3=  bgp.sbjwplnam as begraaf, Trim(Mid(ovl.sbjovldat,7,2)&'-'&Mid(ovl.sbjovldat,5,2)&'-'&Mid(ovl.sbjovldat,1,4)) as ovldat,
SQL4=  Trim(rhb.sbjvlt&' '&Trim(rhb.sbjvvg&' '&rhb.sbjnam)) as Namopm, Trim(MID(rhb.sbjpttkde, 1,4)&' '&MID(rhb.sbjpttkde, 5,2)) as pttkde,
SQL5=  rhb.*, plc.*, positie.* , art.artbdr as plip, rhb.sbjges as rhbgesl
SQL6=  FROM SBJ RHB, PLC, OBJ positie, SBJ OVL, ART, SBJ BGP 
SQL7=  where plcidn=positie.objplcidn 
SQL8=  and plcrhbidn=rhb.sbjidn 
SQL9=  and ovl.sbjidn=positie.objsbjidn 
SQL10=  and art.ARTPLCIDN IN (SELECT positie.OBJPLCIDN FROM obj WHERE (OBJPLCIDN > '') AND (OBJIDN = :objidn)) 
SQL11=  and bgp.sbjidn=plc.plcbgpidn 
SQL12=  and positie.objidn=:objidn

Base=obj
[Parameters-0]
OBJIDN=String



