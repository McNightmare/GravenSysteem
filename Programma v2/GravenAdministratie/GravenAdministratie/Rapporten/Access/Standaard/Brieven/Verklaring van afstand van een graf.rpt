[Query-0]
SQL1=  select Trim(Mid(obj.objvandat,7,2)&'-'&Mid(obj.objvandat,5,2)&'-'&Mid(obj.objvandat,1,4)) as vandat, 
SQL2=  Trim(Mid(obj.objtemdat,7,2)&'-'&Mid(obj.objtemdat,5,2)&'-'&Mid(obj.objtemdat,1,4)) as temdat, 
SQL3=  Trim(Mid(ovl.sbjgebdat,7,2)&'-'&Mid(ovl.sbjgebdat,5,2)&'-'&Mid(ovl.sbjgebdat,1,4)) as gebdat, 
SQL4=  Trim(rhb.sbjvlt&' '&Trim(rhb.sbjvvg&' '&rhb.sbjnam)) as Namopm, Trim(MID(rhb.sbjpttkde, 1,4)&' '&MID(rhb.sbjpttkde, 5,2)) as pttkde,
SQL5=  rhb.*, plc.*, obj.*, rhb.sbjges as rhbgesl, ovl.sbjgebplt as gebplt , Trim(ovl.sbjvlt&' '&Trim(ovl.sbjvvg&' '&ovl.sbjnam)) as ovlnam
SQL6=  FROM SBJ RHB, PLC, OBJ, SBJ OVL
SQL7=  where plcidn=objplcidn and plcrhbidn=rhb.sbjidn and ovl.sbjidn=obj.objsbjidn
SQL8=  and objidn=:objidn
SQL9=  and plcidn=:plcidn

