SQL1=  SELECT usr.*, Trim(sbj.sbjvlt&' '&Trim(sbj.sbjvvg&' '&sbj.sbjnam)) as Namopm, bgp.sbjnam as begraaf,
SQL2=  Trim(sbj.sbjvvg&' '&sbj.sbjnam) as Namkort, Trim(Mid(OBJTEMDAT,7,2)&'-'&Mid(OBJTEMDAT,5,2)&'-'&Mid(OBJTEMDAT,1,4)) as einddat, sbj.sbjtel as tel, sbj.sbjsof as bsn, plc.plcpltidn as kosttype, Trim(Mid(obj.objbgrdat,7,2)&'-'&Mid(obj.objbgrdat,5,2)&'-'&Mid(obj.objbgrdat,1,4)) as bgrdat,
SQL3=  (sbj.sbjges) as rhbgesl, Trim(MID(sbj.sbjpttkde, 1,4)&' '&MID(sbj.sbjpttkde, 5,2)) as pttkde, * 
