SQL1=  select pltnam, usr.*, ovl.sbjovldat, ovl.sbjgebdat, rhb.sbjsttnam, rhb.sbjwplnam, rhb.sbjhuinum, rhb.sbjhuilet, rhb.sbjhuitvg ,Trim(Mid(objvandat,7,2)&'-'&Mid(objvandat,5,2)&'-'&Mid(objvandat,1,4)) as vandat, Trim(rhb.sbjvlt&' '&Trim(rhb.sbjvvg&' '&rhb.sbjnam)) as Namopm, Trim(ovl.sbjvlt&' '&Trim(ovl.sbjvvg&' '&ovl.sbjnam)) as Namobj,
SQL2=  Trim(Mid(ovl.sbjgebdat,7,2)&'-'&Mid(ovl.sbjgebdat,5,2)&'-'&Mid(ovl.sbjgebdat,1,4)) as gebdat, Trim(Mid(OBJTEMDAT,7,2)&'-'&Mid(OBJTEMDAT,5,2)&'-'&Mid(OBJTEMDAT,1,4)) as einddat, Trim(Mid(ovl.sbjovldat,7,2)&'-'&Mid(ovl.sbjovldat,5,2)&'-'&Mid(ovl.sbjovldat,1,4)) as ovldat,Trim(MID(rhb.sbjpttkde, 1,4)&' '&MID(rhb.sbjpttkde, 5,2)) as pttkde, plc.*, obj.*, rhb.sbjtel as tel, rhb.sbjsof as bsn, Trim(Mid(obj.objbgrdat,7,2)&'-'&Mid(obj.objbgrdat,5,2)&'-'&Mid(obj.objbgrdat,1,4)) as bgrdat, rhb.sbjges as rhbgesl  
SQL3=  FROM SBJ RHB, SBJ OVL, PLC, OBJ, USR, PLT 
SQL4=  where plcidn=objplcidn and plcrhbidn=rhb.sbjidn and plcpltidn=pltidn and objsbjidn=ovl.sbjidn
