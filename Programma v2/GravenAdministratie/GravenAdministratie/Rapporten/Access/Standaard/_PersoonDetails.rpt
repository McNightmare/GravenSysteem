[Report]
Name=Persoon Details
TemplateName=_PersoonDetails.rtf
SQLCompatibility=MSAccess

[Query-0]
SQL=SELECT * FROM SBJ WHERE SBJIDN = :SBJIDN
Base=SBJ
[Parameters-0]
SBJIDN=STRING|

[Query-1]
SQL=select DOCIDN, DOCNAM, DOCBDR, DOCDOTIDN, DOCDOSIDN, DOCBPLIDN, DOCINSDAT from DOC_TOTAL where DOCBPLIDN = :SBJIDN ORDER BY DOCINSDAT DESC
Base=DOC
[Parameters-1]
SBJIDN=STRING|

[Query-2]
SQL=select ARTIDN, ARTNAM, ARTBDR, ARTOBJIDN, ARTPLCIDN, ARTSRTIDN, ARTINSDAT, ARTBEGDAT, DORDOCIDN from ART_DOR where ARTBPLIDN = :ARTSBJIDN ORDER BY ARTINSDAT DESC
Base=ART
[Parameters-2]
ARTSBJIDN=STRING|

[Query-3]
SQL=select PLCIDN, PLCUSRKEY, PLCNAM, iif(plcresidn, '', 'X') as plcresind, iif(plcrhbidn, '', 'X') as plcrhbind, PLCRESVAN, PLCRESTEM, OBJIDN, OBJPOS, OBJNAM, OBJVANDAT, OBJTEMDAT, OBJBONIDN from PLC_OBJ_RHB where (PLCRHBIDN = :SBJIDN) OR (PLCRESIDN = :SBJIDN)     
Base=PLC
[Parameters-3]
SBJIDN=STRING|

[Authorization]
Code=3CA1C5CF
