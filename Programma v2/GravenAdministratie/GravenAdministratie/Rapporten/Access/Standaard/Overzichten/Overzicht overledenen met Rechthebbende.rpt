[Query-0]
SQL=SELECT sbj.*, (Mid(sbjovl.sbjovldat,7,2)&'-'&Mid(sbjovl.sbjovldat,5,2)&'-'&Mid(sbjovl.sbjovldat,1,4)) as ovldat, obj.objnam, plc.plcnam FROM SBJ AS SBJ, SBJ AS SBJOVL, OBJ, PLC WHERE SBJ.SBJIDN=PLCRHBIDN AND SBJOVL.SBJIDN=OBJSBJIDN and SBJOVL.SBJOVLDAT>=:VANDAT AND SBJOVL.SBJOVLDAT<=:TEMDAT AND OBJPLCIDN=PLCIDN ORDER BY OBJNAM
Base=SBJ
