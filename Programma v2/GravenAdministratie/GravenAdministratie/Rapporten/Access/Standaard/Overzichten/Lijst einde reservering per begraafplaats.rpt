[Query-0]
SQL=SELECT *,Trim(MID(PLCRESTEM,7,2)&'-'&MID(PLCRESTEM,5,2)&'-'&MID(PLCRESTEM,1,4)) as ENDDAT FROM PLC_OBJ_RHB_BGP_ALL WHERE PLCRESTEM>=:VANDAT AND PLCRESTEM<=:TEMDAT AND PLCBGPIDN=:PLCBGPIDN
Base=SBJ
[Parameters-0]
PLCBGPIDN=STRINGCOMBOBOX|Begraafplaats
VANDAT=DATENUM|Datum einde reservering vanaf
TEMDAT=DATENUM|Datum einde reservering t/m

[PLCBGPIDN]
TableName=SBJ, SBK WHERE SBKSBJIDN=SBJIDN AND SBKSBTIDN='BGP'
ValueFieldName=SBJIDN
NameFieldName=SBJNAM
OrderFieldName=SBJNAM

