[Query-0]
SQL=SELECT sbjovl.*, (Mid(sbjovl.sbjovldat,7,2)&'-'&Mid(sbjovl.sbjovldat,5,2)&'-'&Mid(sbjovl.sbjovldat,1,4)) as ovldat, obj.objnam, plc.plcnam FROM SBJ AS SBJOVL, OBJ, PLC WHERE  SBJOVL.SBJIDN=OBJSBJIDN and SBJOVL.SBJOVLDAT>=:VANDAT AND SBJOVL.SBJOVLDAT<=:TEMDAT AND OBJPLCIDN=PLCIDN ORDER BY OBJNAM
