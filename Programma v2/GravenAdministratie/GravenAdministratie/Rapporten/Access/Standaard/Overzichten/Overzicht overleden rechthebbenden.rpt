[Query-0]
SQL=SELECT SBJ.*, Trim(Mid(sbj.sbjovldat,7,2)&'-'&Mid(sbj.sbjovldat,5,2)&'-'&Mid(sbj.sbjovldat,1,4)) as ovldat, Trim(Mid(sbj.sbjgebdat,7,2)&'-'&Mid(sbj.sbjgebdat,5,2)&'-'&Mid(sbj.sbjgebdat,1,4)) as gebdat FROM SBK, SBJ where SBKSBTIDN = 'OVL' and SBJIDN = SBKSBJIDN and SBJOVLDAT>=:VANDAT AND SBJOVLDAT<=:TEMDAT and sbksbjidn in (select intsbk.sbksbjidn from sbk intsbk where sbksbtidn ='RHB' and intsbk.sbksbjidn = sbk.sbksbjidn) order by sbjnam
Base=SBJ
